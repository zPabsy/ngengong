      "source": [
        "# üåê Launch Forge with Pinggy Tunnel"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {},
      "source": [
        "from multiprocessing import Process\n",
        "import sys\n",
        "import time\n",
        "from IPython import get_ipython\n",
        "\n",
        "# Create log file\n",
        "with open('log.txt', 'w') as f:\n",
        "    f.write('')\n",
        "\n",
        "def run_app():\n",
        "    cmd = \"python /root/stable-diffusion-webui-forge/launch.py & ssh -o StrictHostKeyChecking=no -p 80 -R0:localhost:7860 a.pinggy.io > log.txt 2>&1\"\n",
        "    get_ipython().system(cmd)\n",
        "\n",
        "def print_url():\n",
        "    print(\"‚è≥ Waiting for tunnel URL...\")\n",
        "    while True:\n",
        "        time.sleep(3)\n",
        "        with open('log.txt', 'r') as file:\n",
        "            content = file.read()\n",
        "            if '.pinggy.link' in content:\n",
        "                start_index = content.find('http')\n",
        "                end_index = content.find('.pinggy.link', start_index)\n",
        "                if start_index != -1 and end_index != -1:\n",
        "                    url = content[start_index:end_index + len('.pinggy.link')]\n",
        "                    print(f\"\\n‚úÖ Tunnel ready: {url}\\n\")\n",
        "                    break\n",
        "\n",
        "p_app = Process(target=run_app)\n",
        "p_url = Process(target=print_url)\n",
        "p_app.start()\n",
        "p_url.start()\n",
        "p_app.join()\n",
        "p_url.join()"
      ],
      "execution_count": null,
      "outputs": []
    }
